<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zcls_pen_project.First" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
		
		<Table items="{/Record}" id="tab_IDD">
		<columns>
		<Column visible="false">
      <Text design="bold" text="Category" />
     </Column>
		<Column>
      <Text design="bold" text="Name" />
     </Column>
		<Column>
      <Text design="bold" text="Age" />
     </Column>
     <Column>
      <Text design="bold" text="Mobile" />
     </Column>
     <Column>
      <Text design="bold" text="Town" />
     </Column>
		
		</columns>
		
		<ColumnListItem>
     <cells>
      <Text text="{recog}" />
      <Text text="{name}" />
      <Text text="{age}" />
      <Text text="{mob}" />
      <Text text="{town}" />
     </cells>
    </ColumnListItem>
		
		</Table>
	
		</content>
	</Page>
</core:View>