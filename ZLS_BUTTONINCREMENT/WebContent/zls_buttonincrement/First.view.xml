<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zls_buttonincrement.First" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
		
		<Table id="idProductsTable" items="{/Product}">
		
		<columns>
			<Column>
				<Text text="PRODUCT ID" />
			</Column>
			<Column>
				<Text text="Name" />
			</Column>
			<Column>
				<Text text="Quantity" />
			</Column>
			<Column>
				<Text text="Uintprice" />
			</Column>
			<Column>
				<Text text="Totalamount" />
			</Column>
			<Column>
				<Text text="Incrementvalue" />
			</Column>
			<Column>
				<Text text="GST" />
			</Column>
			
		</columns>
				<items>
			<ColumnListItem>
				<cells>
										<Text text="{ProductID}" />
										<Text text="{Name}" />
										<Text text="{Quantity}" />
										<Text text="{value}" id="id1"/>
										<Text text="{Total}" />
										<Button text="{InValue}" press="onPress"/>
										<Text text="{tax}" />
					
				</cells>
			</ColumnListItem>
		</items>
		
	</Table>
	
		</content>
	</Page>
</core:View>