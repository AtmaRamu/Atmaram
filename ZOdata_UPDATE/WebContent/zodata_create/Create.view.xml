<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zodata_create.Create" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
	     <Label text="Productid"/>
	     <ComboBox id="idCombo" items="{/ProductSet}">
	     <core:Item text="{ProductID}"/>
	     </ComboBox>
	     <Label text="ProductName"/>
	     <Input id="idProdName"/>
		</content>
		<footer>
		<Toolbar>
		<Button text="Update" press="onUpdate"/>
		</Toolbar>
		
		</footer>
	</Page>
</core:View>